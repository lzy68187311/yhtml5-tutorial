  "scripts": {
    "prestart": "clear",
    "start": "npm i && npm run build && npm run dev",
    "prebuild": "clear",
    "build": "NODE_ENV=production  webpack --progress --hide-modules --colors --config build/webpack.config.js",
    "build-debug": "NODE_ENV=production  webpack-dashboard -- webpack --progress --display-modules --profile --colors --config build/webpack.config.js",
    "predev": "clear",
    "dev": "NODE_ENV=production  webpack-dashboard -- webpack-dev-server --open  --config build/webpack.config.js",
    "dev-react": "webpack-dev-server --open  --config build/webpack.config.react.js",
    "postdev": "clear",
    "pregit": "clear && git status",
    "git": "git add -A && git commit -m 'auto update' && git checkout master && git merge dev && git push --all && git checkout dev",
    "postgit": "clear && git status",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
